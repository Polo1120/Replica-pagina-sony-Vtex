{
  "flex-layout.col#menu-top-mobile": {
    "children": ["menu#menu-top"],
    "props": {
      "width": "3%"
    }
  },
  "menu#menu-top": {
    "children": ["menu-item#ayuda-mobile", "menu-item#more-options-mobile"],
    "props": {
      "blockClass": "menu-top-mobile"
    }
  },
  "menu-item#ayuda-mobile": {
    "props": {
      "id": "ayuda-menu",
      "type": "custom",
      "itemProps": {
        "type": "external",
        "href": "0 0 16 16",
        "noFollow": true,
        "tagTitle": "ayuda-menu",
        "text": "Ayuda"
      },
      "blockClass": "menu-ayuda-mobile"
    }
  },
  "menu-item#more-options-mobile": {
    "props": {
      "iconProps": {
        "id": "icon-puntos",
        "size": 20,
        "viewBox": "0 0 210 210"
      },
      "blockClass": "menu-more-options-mobile"
    },
    "blocks": ["submenu#submenu-mobile-more-options-mobile"]
  },
  "submenu#submenu-mobile-more-options-mobile": {
    "children": ["menu#submenu-mobile-more-options"]
  },
  "menu#submenu-mobile-more-options": {
    "title": "Sub menu more options",
