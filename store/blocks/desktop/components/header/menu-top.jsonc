{
  "flex-layout.row#menu-top": {
    "title": "Menu top",
    "children": ["flex-layout.col#menu"],
    "props": {
      "blockClass": "menu-top2"
    }
  },
  "flex-layout.col#logo-telef": {
    "title": "logo telef",
    "children": ["icon#telefono"],
